{
  "name": "github-watcher",
  "version": "1.0.0",
  "description": "GitHub followers and following watcher",
  "type": "module",
  "scripts": {
    "dev": "node --import @swc-node/register/esm-register src/main.ts",
    "build": "tsc",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "list-followers": "node --import @swc-node/register/esm-register src/main.ts ghtool list-followers",
    "list-following": "node --import @swc-node/register/esm-register src/main.ts ghtool list-following"
  },
  "keywords": [],
  "author": "Kai",
  "license": "MIT",
  "dependencies": {
    "@effect/cli": "^0.59.8",
    "@effect/platform-node": "^0.76.16",
    "@octokit/rest": "^21.1.1",
    "@swc-node/register": "^1.10.10",
    "dotenv": "^16.5.0",
    "effect": "^3.14.8"
  },
  "devDependencies": {
    "@effect/vitest": "^0.20.8",
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  }
}
